{
  "name": "dash",
  "version": "0.0.1",
  "license": "NONE",
  "scripts": {
    "clear": "rd /s /q dist .parcel-cache",
    "watch": "yarn clear && parcel watch src/{main.html,options.html,popup.html,background.js} --dist-dir dist/src --public-url ./ -t node --bundle-node-modules",
    "build": "yarn clear && parcel build src/{main.html,options.html,popup.html,background.js} --dist-dir dist/src --public-url ./",
    "c": "rm -rf dist .parcel-cache",
    "w": "yarn c && parcel watch src/{main.html,options.html,popup.html,background.js} --dist-dir dist/src --public-url ./ -t node --bundle-node-modules",
    "b": "yarn c && parcel build src/{main.html,options.html,popup.html,background.js} --dist-dir dist/src --public-url ./"
  },
  "volta": {
    "node": "14.15.4",
    "yarn": "1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "autoprefixer": "^10.2.4",
    "parcel": "^2.0.0-nightly.558",
    "parcel-reporter-static-files-copy": "^1.1.1",
    "postcss": "^8.2.4",
    "postcss-modules": "^4.0.0",
    "tailwindcss": "^2.0.2"
  },
  "dependencies": {
    "preact": "^10.5.12"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  }
}